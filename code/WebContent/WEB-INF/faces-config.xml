<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>fileUploadView</managed-bean-name>
		<managed-bean-class>com.iiitb.ddf.FileUploadView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dataTableView</managed-bean-name>
		<managed-bean-class>com.iiitb.ddf.DataTableView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>selectCriteria</managed-bean-name>
		<managed-bean-class>com.iiitb.ddf.SelectCriteria</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>

</faces-config>
